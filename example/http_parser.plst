extern
  memchr
    <-
      ptr(i8),
      i32,
      u64,
    ->
      ptr(i8)

struct
  chunk_t
    data: ptr(i8),
    size: u64,

enum
  partial_http_result_t
    method_chunk(chunk_t),
    method_end(),
    request_uri_chunk(chunk_t),
    request_uri_end(),
    header_key_chunk(chunk_t),
    header_key_end(),
    header_value_chunk(chunk_t),
    header_value_end(),
    body_chunk(chunk_t),
    body_end(),

enum
  partial_result_t
    continue(),
    part(partial_http_result_t),

