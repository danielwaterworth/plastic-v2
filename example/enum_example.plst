enum
  test
    foo(),
    bar(i32),

extern
  puts
    <-
      ptr(i8),
    ->
      void

extern
  putint
    <-
      i32,
    ->
      void

define
  main
    = {
      let b = foo();
      match b {
        foo() {
          puts("foo\00");
        }
        bar(i) {
          putint(i);
        }
      }
      puts("end\00");
      return void;
    }
